{
	"name": "linkt",
	"version": "1.5.4",
	"keywords": [
		"peerjs",
		"webrtc",
		"p2p",
		"rtc"
	],
	"description": "PeerJS client",
	"homepage": "https://peerjs.com",
	"bugs": {
		"url": "https://github.com/peers/peerjs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/peers/peerjs"
	},
	"license": "MIT",
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/peer"
	},
	"collective": {
		"type": "opencollective",
		"url": "https://opencollective.com/peer"
	},
	"files": [
		"dist/*"
	],
	"sideEffects": [
		"src/global.ts",
		"src/utils/supports.ts"
	],
	"main": "dist/bundler.cjs",
	"module": "dist/bundler.mjs",
	"browser-minified": "dist/linkt.min.js",
	"browser-unminified": "dist/linkt.js",
	"types": "dist/types.d.ts",
	"engines": {
		"node": ">= 14"
	},
	"scripts": {
		"generate:version": "node scripts/generate-version.js",
		"prebuild": "npm run generate:version",
		"clean": "rimraf ./lib ./build",
		"build:ts": "tsc",
		"build:rollup": "rollup -c",
		"build": "npm run clean && npm run build:ts && npm run build:rollup",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run build",
		"test": "jest",
		"test:watch": "jest --watch",
		"coverage": "jest --coverage --collectCoverageFrom=\"./src/**\"",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"e2e": "npm run build && node e2e/run-e2e.js",
		"e2e:bstack": "wdio run e2e/wdio.bstack.conf.ts"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^28.0.3",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-terser": "^0.4.4",
		"@swc/core": "^1.3.27",
		"@swc/jest": "^0.2.24",
		"@types/jasmine": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@wdio/browserstack-service": "^8.11.2",
		"@wdio/cli": "^8.11.2",
		"@wdio/globals": "^8.11.2",
		"@wdio/jasmine-framework": "^8.11.2",
		"@wdio/local-runner": "^8.11.2",
		"@wdio/spec-reporter": "^8.11.2",
		"@wdio/types": "^8.10.4",
		"http-server": "^14.1.1",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"mock-socket": "^9.0.0",
		"prettier": "^3.0.0",
		"rollup": "^4.41.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.0",
		"wdio-geckodriver-service": "^5.0.1"
	},
	"dependencies": {
		"@msgpack/msgpack": "^2.8.0",
		"eventemitter3": "^4.0.7",
		"peerjs-js-binarypack": "^2.1.0",
		"webrtc-adapter": "^9.0.0"
	},
	"alias": {
		"process": false,
		"buffer": false
	}
}
